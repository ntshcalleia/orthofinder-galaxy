<tool id="toolshed_orthofinder.fd" name="OrthoFinder">
  <description>Dynamic OrthoFinder</description>
  <command>python '${__tool_directory__}/tool.py' '$specie_tree' '$output2' '$output3' '$output4' '$output5' '$input1' '$input2' </command>
  <inputs>
    <param format="fasta" name="input1" type="data" label="Source file"/>
	  <param format="fasta" name="input2" type="data" label="Source file"/>
  </inputs>
  <outputs>
    <data format="csv" name="specie_tree" label="specie_tree" />
	  <data format="tabular" name="output2" label="orthogroups" />
    <data format="tabular" name="output3" label="" />
    <data format="tabular" name="output4" label="" />
    <data format="tabular" name="output5" label="" />
  </outputs>

